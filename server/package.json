{
  "name": "project-manager-server",
  "version": "1.0.0",
  "description": "Backend API para gesti√≥n de proyectos personales",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:optimized": "node start-simple.js",
    "dev:no-limit": "DISABLE_RATE_LIMIT=true nodemon src/index.ts",
    "dev:simple": "set NODE_ENV=development && set DISABLE_RATE_LIMIT=true && nodemon src/index.ts",
    "start-dev": "node start-dev.js",
    "monitor": "node monitor-requests.js",
    "dev:monitor": "concurrently \"npm run start-dev\" \"npm run monitor\"",
    "build": "npx --package=typescript tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "supabase"
  ],
  "author": "David Rey",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
